    "version": "0.2.0",
    "configurations": [
        {
            "name": "dataget Program",
            "program": "${workspaceFolder}/dataget.js",
            "args": [
                "-t",
                "Calfiornia-Vaccine-Overtime-Table"
            ],
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "name": "t2 run",
            "program": "${workspaceFolder}/t2.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "name": "Run Mapper",
            "program": "${workspaceFolder}/Mapper.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "name": "Run Watcher",
            "program": "${workspaceFolder}/watcher.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
    ]
}